package com.qa.garage.files;

public class Bike extends Vehicle{
	
	// Creating attributes 
	private String type;
	private int topSpeed;
	private float wheelSize;
	// Constructor
	public Bike(String id, String model, boolean electric, String type, int topSpeed, float wheelSize) {
		super(id, model, electric);
		this.type = type;
		this.topSpeed = topSpeed;
		this.wheelSize = wheelSize;
	}
	// Getters and Setters
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public int getTopSpeed() {
		return topSpeed;
	}
	public void setTopSpeed(int topSpeed) {
		this.topSpeed = topSpeed;
	}
	public float getWheelSize() {
		return wheelSize;
	}
	public void setWheelSize(float wheelSize) {
		this.wheelSize = wheelSize;
	}
	

	
	

}
