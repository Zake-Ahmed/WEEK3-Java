package com.qa.week3.day2.parameter;

public class Exercises {
	public static int blackJack(int x , int y) {
		if(x<=21 && y<=21) {
			if(x>y) {
				return x;
			} else {
				return y;
			}
		} else {
			return 0;
		}
	}
	public static int uniqueSum(int x, int y , int z) {
		if (x!= y && x != z && y != z) {
			return x+y+z;
		} else if (x != y && x == z) {
			return x+y;
		} else if (x==y && x != z) {
			return x+z;
		} else if (y==z) {
			return z;
		} else {
			return 0;
		}
	}

}
