package com.qa.week3.day2.parameter;

import java.util.Random;

public class Loop {
	public static void moneySpend(int s, float e, int l ) {
		while (s>l) {
			System.out.println("New Balence " + s);
			s -= e;;
		}
		System.out.println("STOP SPENDING MONEY!!");
	}
	public static void moneySpend2(int s, int l ) {
		while (s>l) {
			int e=  new Random().nextInt(26) + 10;
			System.out.println("New Balence " + s);
			s -= e;
			
		}
		System.out.println("STOP SPENDING MONEY!!");
	}

}
